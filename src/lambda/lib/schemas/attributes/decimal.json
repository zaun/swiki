      "format": "url"
    },

    "type": {
      "enum": ["DECIMAL"]
    },

    "value": {
      "type": "string",
      "description": "numbers only",
      "pattern": "^[+-]?(\\d*\\.)?\\d+$"
    }
  },
  "required": ["name", "type", "value"]
}
